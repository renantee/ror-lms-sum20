<h1><%= @course.name %></h1>
<hr />
<h2>Users</h2>
<ul>
  <% @course.users.each do |user| %>
    <li>
      <%= link_to user.full_name, user %>
    </li>
  <% end %>
</ul>
<br />
<%= link_to 'Edit Course', edit_course_path(@course), class: 'ui info button' %>
<%= link_to 'Delete Course', @course, method: :delete, class: 'ui red button' %>